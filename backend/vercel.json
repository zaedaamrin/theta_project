{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/index.js",
        "use": "@vercel/node",
        "config": {
        "buildCommand": "npm install && npm run start"
      }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/api/index.js"
      }
    ]
  }  